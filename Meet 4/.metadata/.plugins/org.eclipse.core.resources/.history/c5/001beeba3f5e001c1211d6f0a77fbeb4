package makhluk_hidup;

public class Hewan {
	int jumlahKaki;
	String namaHewan;
	public Hewan(int jumlahKaki, String namaHewan){
		this.jumlahKaki = jumlahKaki;
		this.namaHewan = namaHewan;
	}
	public void gerak(){
		System.out.println(namaHewan + " bergerak...");
		System.out.println(namaHewan + " mempunyai kaki sebanyak " + jumlahKaki);
	}
}
