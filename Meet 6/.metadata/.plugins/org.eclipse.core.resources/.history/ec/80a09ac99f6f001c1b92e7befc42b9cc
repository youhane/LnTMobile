
public class Mahasiswa {
	private String nama;
	private int usia;
	private String nim;
	private String lokasi;
	public String getNama() {
		return nama;
	}
	public void setNama(String nama) {
		this.nama = nama;
	}
	public int getUsia() {
		return usia;
	}
	public void setUsia(int usia) {
		this.usia = usia;
	}
	public String getNim() {
		return nim;
	}
	public void setNim(String nim) {
		this.nim = nim;
	}
	public String getLokasi() {
		return lokasi;
	}
	public void setLokasi(String lokasi) {
		this.lokasi = lokasi;
	}
	public Mahasiswa(String nama, int usia, String nim, String lokasi) {
		this.nama = nama;
		this.usia = usia;
		this.nim = nim;
		this.lokasi = lokasi;
	}
	
}
