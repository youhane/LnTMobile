		Kendaraan supra = new Motor();
		Motor vario = new Motor();
		supra.setNama("Supra X");
		supra.setJumlahRoda(2);
		supra.setKapasitasBensin(150);
		supra.setKecepatanMaksimum(160);
		
		vario.setNama("Vario");
		vario.setJumlahRoda(2);
		vario.setKecepatanMaksimum(180);
		vario.setKecepatanMaksimum(150);
		
		supra.jalan(supra.getNama());
		supra.rem(supra.getNama());
		supra.isiBensin(supra.getNama(), supra.getKapasitasBensin());
		supra.ngebut(supra.getNama(), supra.getKecepatanMaksimum());
		
		vario.jalan(vario.getNama());
		vario.rem(vario.getNama());
		vario.isiBensin(vario.getNama(), vario.getKapasitasBensin());
		vario.ngebut(vario.getNama(), vario.getKecepatanMaksimum());
	}

