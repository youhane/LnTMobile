package makhluk_hidup;

// INHERITANCE
public class Dosen extends Manusia{
	private String kode;
	private String kampus;
	
	public String getKode() {
		return kode;
	}
	public void setKode(String kode) {
		this.kode = kode;
	}
	
	public String getKampus() {
		return kampus;
	}
	public void setKampus(String kampus) {
		this.kampus = kampus;
	}
	
	// OVERRIDING
	public void bicara() {
		System.out.println(super.getNama() + " mengajar di Binus " + kampus);
	}
	
}
